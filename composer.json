{
	"name": "tyea/aviator",
	"description": "A Symfony based microframework for prototyping applications",
	"keywords": [
		"aviator",
		"symfony",
		"microframework",
		"prototyping"
	],
	"homepage": "https://github.com/tyea/aviator",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Yeadon",
			"homepage": "http://www.tomyeadon.com"
		}
	],
	"require": {
		"php": ">=7.2",
		"ext-json": "*",
		"ext-pdo": "*",
		"symfony/http-foundation": "^5.3",
		"symfony/routing": "^5.3",
		"twig/twig": "^3.3"
	},
	"autoload": {
		"psr-4": {
			"Tyea\\Aviator\\": "src/"
		},
		"files": [
			"src/constants.php",
			"src/functions.php"
		]
	}
}